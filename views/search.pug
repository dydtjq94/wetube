extends layouts/main
include mixin/videoBlock

block content
    .search__header
        h3 #{searchingBy}로 검색한 결과입니다.
    .search__videos
        if videos.length === 0 
            h5 찾고 있는 비디오가 없어요 :(
        each item in videos
            +videoBlock({
                    id:item.id,
                    title:item.title,
                    views:item.views,
                    videoFile:item.fileUrl,
                    })